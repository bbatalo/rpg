  <eClassifiers xsi:type="ecore:EClass" name="CooldownChange" eSuperTypes="#//Change">
    <eAnnotations source="http://www.eclipse.org/emf/2002/Ecore">
      <details key="constraints" value="MeaningfullCooldownChange ProperAbilityReference"/>
    </eAnnotations>
    <eAnnotations source="http://www.eclipse.org/emf/2002/Ecore/OCL/Pivot">
      <details key="MeaningfullCooldownChange" value="&#xA;&#x9;&#x9;&#x9;self.talent &lt;> null or self.ability &lt;> null and&#xA;&#x9;&#x9;&#x9;(self.talent &lt;> null implies self.ability = null) and&#xA;&#x9;&#x9;&#x9;(self.ability &lt;> null implies self.talent = null)"/>
      <details key="MeaningfullCooldownChange$message" value="'A CooldownChange should reference either talent or ability.'"/>
      <details key="ProperAbilityReference" value="&#xA;&#x9;&#x9;&#x9;--TODO: should be tested&#xA;&#x9;&#x9;&#x9;self.effect.ability.node.branch.characterClass.branches->collect(nodes)->collect(abilities)->includes(self.ability) or&#xA;&#x9;&#x9;&#x9;self.effect.ability.node.branch.characterClass->closure(parentClasses)->exists(charClass | charClass.branches->collect(nodes)->collect(abilities->includes(self.ability)))"/>
      <details key="ProperAbilityReference$message" value="'Can only reference abilities of host or parent class.'"/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EReference" name="ability" eType="#//Ability"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="talent" eType="#//Talent"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="cooldownTalent" eType="#//CooldownTalent"
        eOpposite="#//CooldownTalent/cooldownChanges"/>
  </eClassifiers>
