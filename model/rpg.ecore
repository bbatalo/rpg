  <eClassifiers xsi:type="ecore:EClass" name="Node">
    <eAnnotations source="http://www.eclipse.org/emf/2002/Ecore">
      <details key="constraints" value="RootNodeChildren StandardNodeChildren StandaloneNodeConnections SelfReference CircularReference ProperReference ActivationCondition ActivationIntegrity MinimalNodeActivation MaximalNodeActivation UniqueNodeActivationLevels ValidBranchTypeImplication"/>
    </eAnnotations>
    <eAnnotations source="http://www.eclipse.org/emf/2002/Ecore/OCL/Pivot">
      <details key="RootNodeChildren" value="&#xA;&#x9;&#x9;&#x9;self.nodeType = NodeType::ROOT implies &#xA;&#x9;&#x9;&#x9;(self.childNodes->forAll(node | node.nodeType = NodeType::STANDARD) and self.parentNodes->size() = 0)"/>
      <details key="RootNodeChildren$message" value="'ROOT nodes can have only STANDARD nodes for children and can not have any parents.'"/>
      <details key="StandardNodeChildren" value="&#xA;&#x9;&#x9;&#x9;self.nodeType = NodeType::STANDARD implies &#xA;&#x9;&#x9;&#x9;(self.childNodes->forAll(node | node.nodeType = NodeType::STANDARD) and&#xA;&#x9;&#x9;&#x9;self.parentNodes->forAll(node | node.nodeType = NodeType::ROOT or node.nodeType = NodeType::STANDARD))"/>
      <details key="StandardNodeChildren$message" value="'STANDARD nodes can have only STANDARD nodes for children and ROOT or STANDARD nodes for parents'"/>
      <details key="StandaloneNodeConnections" value="&#xA;&#x9;&#x9;&#x9;self.nodeType = NodeType::STANDALONE implies self.parentNodes->size() = 0 and self.childNodes->size() = 0"/>
      <details key="StandaloneNodeConnections$message" value="'STANDALONE nodes cannot have parent or child nodes.'"/>
      <details key="SelfReference" value="&#xA;&#x9;&#x9;&#x9;self.childNodes->forAll(node | node &lt;> self) and self.parentNodes->forAll(node | node &lt;> self)"/>
      <details key="SelfReference$message" value="'Nodes cannot reference themselves as either parent or child. '"/>
      <details key="CircularReference" value="&#xA;&#x9;&#x9;&#x9;self->closure(childNodes)->excludes(self) and self->closure(parentNodes)->excludes(self)"/>
      <details key="CircularReference$message" value="'Nodes cannot form circular references to themselves through parent and child relationships.'"/>
      <details key="ProperReference" value="&#xA;&#x9;&#x9;&#x9;self.childNodes->forAll(node | self.branch.nodes->includes(node)) and self.parentNodes->forAll(node | self.branch.nodes->includes(node))"/>
      <details key="ProperReference$message" value="'Nodes can only reference nodes within the same Branch.'"/>
      <details key="ActivationCondition" value="&#xA;&#x9;&#x9;&#x9;self.nodeType = NodeType::STANDARD implies &#xA;&#x9;&#x9;&#x9;(self.activationLevel >= self.minActivationLevel implies self.parentNodes->exists(parentNode | parentNode.activationLevel >= parentNode.minActivationLevel))"/>
      <details key="ActivationCondition$message" value="'Before a node can be activated, at least one parent must be activated.'"/>
      <details key="ActivationIntegrity" value="&#xA;&#x9;&#x9;&#x9;self.activationLevel &lt;= self.maxActivationLevel"/>
      <details key="ActivationIntegrity$message" value="'Activation level must be cannot exceed maximum activation levels.'"/>
      <details key="MinimalNodeActivation" value="&#xA;&#x9;&#x9;&#x9;self.activationLevel &lt; self.minActivationLevel implies &#xA;&#x9;&#x9;&#x9;self.activation->size() = (self.maxActivationLevel - self.minActivationLevel + 1) and self.activationLevel &lt; self.minActivationLevel"/>
      <details key="MinimalNodeActivation$message" value="'Non-activated nodes must have at least one node activation rule and have activation level below minimum.'"/>
      <details key="MaximalNodeActivation" value="&#xA;&#x9;&#x9;&#x9;self.activationLevel >= self.minActivationLevel implies self.activation->size() = (self.maxActivationLevel - self.activationLevel)"/>
      <details key="MaximalNodeActivation$message" value="'Activated nodes must have proper number of node activations.'"/>
      <details key="UniqueNodeActivationLevels" value="&#xA;&#x9;&#x9;&#x9;self.activation->forAll(n1: NodeActivation, n2: NodeActivation | n1 &lt;> n2 implies n1.level &lt;> n2.level)"/>
      <details key="UniqueNodeActivationLevels$message" value="'All node activations must have unique levels.'"/>
      <details key="ValidBranchTypeImplication" value="&#xA;&#x9;&#x9;&#x9;if self.branch.branchType = BranchType::TALENT then&#xA;&#x9;&#x9;&#x9;&#x9;self.talents->size() > 0 and self.abilities->size() = 0&#xA;&#x9;&#x9;&#x9;else &#xA;&#x9;&#x9;&#x9;&#x9;self.talents->size() = 0 and self.abilities->size() > 0&#xA;&#x9;&#x9;&#x9;endif"/>
      <details key="ValidBranchTypeImplication$message" value="'Branch type dictates what the nodes can contain: abilities or talents.'"/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="lore" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
