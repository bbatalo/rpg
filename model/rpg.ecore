      <details key="constraints" value="ProperEffectLevels MeaningfulEffect InstantiateConstraint"/>
    </eAnnotations>
    <eAnnotations source="http://www.eclipse.org/emf/2002/Ecore/OCL/Pivot">
      <details key="ProperEffectLevels" value="&#xA;&#x9;&#x9;&#x9;self.onLevel >= self.ability.node.minActivationLevel and self.onLevel &lt;= self.ability.node.maxActivationLevel"/>
      <details key="ProperEffectLevels$message" value="'Effects must reference an appropriate node level.'"/>
      <details key="MeaningfulEffect" value="&#xA;&#x9;&#x9;&#x9;if self.effectType = EffectType::INSTANTIATE then&#xA;&#x9;&#x9;&#x9;&#x9;self.instantiate->size() = 1&#xA;&#x9;&#x9;&#x9;else&#xA;&#x9;&#x9;&#x9;&#x9;self.changes->size() > 0&#xA;&#x9;&#x9;&#x9;endif"/>
      <details key="MeaningfulEffect$message" value="'An effect must either instantiate a class or change a stat.'"/>
      <details key="InstantiateConstraint" value="&#xA;&#x9;&#x9;&#x9;self.effectType = EffectType::INSTANTIATE implies self.targetType = TargetType::SELF"/>
      <details key="InstantiateConstraint$message" value="'Instantiate effects can only target SELF.'"/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="tooltip" lowerBound="1"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="effectType" lowerBound="1"
        eType="#//EffectType"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="targetType" lowerBound="1"
        eType="#//TargetType"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="onLevel" lowerBound="1"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="instantiate" eType="#//CharacterClass"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="ability" lowerBound="1"
        eType="#//Ability" eOpposite="#//Ability/effects"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="randomizer" eType="#//Randomizer"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="changes" upperBound="-1"
        eType="#//Change" containment="true" eOpposite="#//Change/effect"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="isLocked" lowerBound="1"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBoolean" defaultValueLiteral="false"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="EffectType">
    <eLiterals name="PASSIVE"/>
    <eLiterals name="ACTIVE" value="1"/>
    <eLiterals name="INSTANTIATE" value="2"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="StatChange" eSuperTypes="#//Change">
    <eAnnotations source="http://www.eclipse.org/emf/2002/Ecore">
      <details key="constraints" value="SelfEffectStatChange SelfTalentStatChange SingularModifierOrder"/>
    </eAnnotations>
    <eAnnotations source="http://www.eclipse.org/emf/2002/Ecore/OCL/Pivot">
      <details key="SelfEffectStatChange" value="&#xA;&#x9;&#x9;&#x9;self.effect.targetType = TargetType::SELF and self.effect &lt;> null implies &#xA;&#x9;&#x9;&#x9;(self.effect.ability.node.branch.characterClass.statistics->includes(self.changesStatistic)&#xA;&#x9;&#x9;&#x9;or self.effect.ability.node.branch.characterClass->closure(parentClasses)->exists(charClass | charClass.statistics->includes(self.changesStatistic)))"/>
      <details key="SelfEffectStatChange$message" value="'If StatChange is a part of SELF Effect, it can reference only statistics in host or parent classes.'"/>
      <details key="SelfTalentStatChange" value="&#xA;&#x9;&#x9;&#x9;self.statTalent &lt;> null implies &#xA;&#x9;&#x9;&#x9;(self.statTalent.node.branch.characterClass.statistics->includes(self.changesStatistic)&#xA;&#x9;&#x9;&#x9;or self.statTalent.node.branch.characterClass->closure(parentClasses)->exists(charClass | charClass.statistics->includes(self.changesStatistic)))"/>
      <details key="SelfTalentStatChange$message" value="'If StatChange is a part of StatTalent, it can reference only statistics in host or parent classes.'"/>
      <details key="SingularModifierOrder" value="&#xA;&#x9;&#x9;&#x9;self.modifiers->forAll(m1, m2 | m1 &lt;> m2 implies m1.priorityOrder &lt;> m2.priorityOrder)"/>
      <details key="SingularModifierOrder$message" value="'Each modifier has to have a unique priority order.'"/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EReference" name="changesStatistic" lowerBound="1"
        eType="#//CharacterStatistic"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="modifiers" upperBound="-1"
        eType="#//Modifier" containment="true" eOpposite="#//Modifier/statChange"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="randomizer" eType="#//Randomizer"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="tick" eType="#//Tick" containment="true"
        eOpposite="#//Tick/statChange"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="statTalent" eType="#//StatTalent"
        eOpposite="#//StatTalent/statChanges"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Modifier">
    <eAnnotations source="http://www.eclipse.org/emf/2002/Ecore">
      <details key="constraints" value="NoMovingLimits"/>
    </eAnnotations>
    <eAnnotations source="http://www.eclipse.org/emf/2002/Ecore/OCL/Pivot">
      <details key="NoMovingLimits" value="&#xA;&#x9;&#x9;&#x9;self.changeType &lt;> ChangeType::MOVE_LIMIT_UP and self.changeType &lt;> ChangeType::MOVE_LIMIT_DOWN and self.changeType &lt;> ChangeType::SET_LIMIT"/>
      <details key="NoMovingLimits$message" value="'A modifier cannot move a statistic limit.'"/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="changeType" eType="#//ChangeType"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="value" lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EDouble"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="basedOnModifier" eType="#//ChangeType"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="priorityOrder" lowerBound="1"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="basedOn" eType="#//CharacterStatistic"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="basedOnValue" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EDouble"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="statChange" lowerBound="1"
        eType="#//StatChange" eOpposite="#//StatChange/modifiers"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="randomizer" eType="#//Randomizer"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="onLevel" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"
        defaultValueLiteral="1"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="exclusive" lowerBound="1"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBoolean" defaultValueLiteral="false"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="targetStatistic" lowerBound="1"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBoolean" defaultValueLiteral="false"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="ChangeType">
    <eLiterals name="ADD"/>
    <eLiterals name="SUBSTRACT" value="1"/>
    <eLiterals name="MULTIPLY" value="2"/>
    <eLiterals name="MOVE_LIMIT_UP" value="3"/>
    <eLiterals name="MOVE_LIMIT_DOWN" value="4"/>
    <eLiterals name="SET_LIMIT" value="5"/>
  </eClassifiers>
