  <eClassifiers xsi:type="ecore:EClass" name="PropagationRequirement">
    <eAnnotations source="http://www.eclipse.org/emf/2002/Ecore">
      <details key="constraints" value="ProperRequiredLevel SelfReference ProperPropagationReference"/>
    </eAnnotations>
    <eAnnotations source="http://www.eclipse.org/emf/2002/Ecore/OCL/Pivot">
      <details key="ProperRequiredLevel" value="&#xA;&#x9;&#x9;&#x9;self.requiredLevel >= self.followsActivationOf.minActivationLevel and self.requiredLevel &lt;= self.followsActivationOf.maxActivationLevel"/>
      <details key="ProperRequiredLevel$message" value="'Required level must be between minimum and maximum possible values of the level.'"/>
      <details key="SelfReference" value="&#xA;&#x9;&#x9;&#x9;self.nodeActivation.node &lt;> self.followsActivationOf"/>
      <details key="SelfReference$message" value="'Cannot reference host Node.'"/>
      <details key="ProperPropagationReference" value="&#xA;&#x9;&#x9;&#x9;self.nodeActivation.node.branch.characterClass.branches->collect(nodes)->includes(self.followsActivationOf) or&#xA;&#x9;&#x9;&#x9;self.nodeActivation.node.branch.characterClass->closure(parentClasses)->exists(charClass | charClass.branches->collect(nodes)->includes(self.followsActivationOf))"/>
      <details key="ProperPropagationReference$message" value="'Referenced Node must be within host class or parent classes, recursively.'"/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="requiredLevel" lowerBound="1"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="followsActivationOf" lowerBound="1"
        eType="#//Node"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="nodeActivation" lowerBound="1"
        eType="#//NodeActivation" eOpposite="#//NodeActivation/requirements"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="ChangeValueType">
    <eLiterals name="VALUE"/>
    <eLiterals name="PERCENTAGE" value="1"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Randomizer">
    <eAnnotations source="http://www.eclipse.org/emf/2002/Ecore">
      <details key="constraints" value="MeaningfulRandomizer ProperStatisticBase"/>
    </eAnnotations>
    <eAnnotations source="http://www.eclipse.org/emf/2002/Ecore/OCL/Pivot">
      <details key="MeaningfulRandomizer" value="&#xA;&#x9;&#x9;&#x9;(self.value > 0 implies self.basedOn = null) and &#xA;&#x9;&#x9;&#x9;(self.basedOn &lt;> null implies self.value = 0)"/>
      <details key="MeaningfulRandomizer$message" value="'Randomizer should have value greater than zero or be based on character statistic.'"/>
      <details key="ProperStatisticBase" value="&#xA;&#x9;&#x9;&#x9;self.basedOn &lt;> null implies&#xA;&#x9;&#x9;&#x9;self.basedOn.minValue >= 0 and self.basedOn.maxValue &lt;= 100 and self.basedOn.maxValue = self.basedOn.startLimitValue"/>
      <details key="ProperStatisticBase$message" value="'Referenced statistics should model chance, therefore values should be between 0 and 100.'"/>
    </eAnnotations>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="value" lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EDouble"
        defaultValueLiteral="0"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="basedOn" eType="#//CharacterStatistic"/>
  </eClassifiers>
