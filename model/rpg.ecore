        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="description" lowerBound="1"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="nodeType" lowerBound="1"
        eType="#//NodeType" defaultValueLiteral="STANDARD"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="childNodes" eType="#//Node"
        eOpposite="#//Node/parentNodes"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="parentNodes" eType="#//Node"
        eOpposite="#//Node/childNodes"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="activation" upperBound="-1"
        eType="#//NodeActivation" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="minActivationLevel" lowerBound="1"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt" defaultValueLiteral="1"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="maxActivationLevel" lowerBound="1"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt" defaultValueLiteral="1"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="activationLevel" lowerBound="1"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt" defaultValueLiteral="0"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="isActivated" lowerBound="1"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBoolean" defaultValueLiteral="false"/>
  </eClassifiers>
