      <details key="ProperNodeActivationLevels$message" value="'All node activations must have levels between min and max activation levels.'"/>
      <details key="MeaningfulActivationCondition" value="&#xA;&#x9;&#x9;&#x9;self.costs->size() > 0 or self.requirements->size() > 0"/>
      <details key="MeaningfulActivationCondition$message" value="'Node activation should have at least one cost or propagation requirement.'"/>
    </eAnnotations>
