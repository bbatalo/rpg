  <eClassifiers xsi:type="ecore:EClass" name="Node">
    <eAnnotations source="http://www.eclipse.org/emf/2002/Ecore">
      <details key="constraints" value="RootNodeChildren RootNodeParents StandardNodeChildren StandardNodeParents StandaloneNodeConnections ActivationIntegrity MinimalNodeActivation MaximalNodeActivation UniqueNodeActivationLevels ProperNodeActivationLevels ProperAbilityLevels ProperEffectLevels"/>
    </eAnnotations>
    <eAnnotations source="http://www.eclipse.org/emf/2002/Ecore/OCL/Pivot">
      <details key="RootNodeChildren" value="self.nodeType = NodeType::ROOT implies self.childNodes->forAll(node | node.nodeType = NodeType::STANDARD)"/>
      <details key="RootNodeChildren$message" value="'ROOT nodes can have only STANDARD nodes for children.'"/>
      <details key="RootNodeParents" value="self.nodeType = NodeType::ROOT implies self.parentNodes->size() = 0"/>
      <details key="RootNodeParents$message" value="'ROOT nodes can not have any parents.'"/>
      <details key="StandardNodeChildren" value="self.nodeType = NodeType::STANDARD implies self.childNodes->forAll(node | node.nodeType = NodeType::STANDARD)"/>
      <details key="StandardNodeChildren$message" value="'STANDARD nodes can have only STANDARD nodes for children.'"/>
      <details key="StandardNodeParents" value="self.nodeType = NodeType::STANDARD implies self.parentNodes->forAll(node | node.nodeType = NodeType::ROOT or node.nodeType = NodeType::STANDARD)"/>
      <details key="StandardNodeParents$message" value="'STANDARD nodes can have only ROOT or STANDARD nodes for parents.'"/>
      <details key="StandaloneNodeConnections" value="self.nodeType = NodeType::STANDALONE implies self.parentNodes->size() = 0 and self.childNodes->size() = 0"/>
      <details key="StandaloneNodeConnections$message" value="'STANDALONE nodes can not have parent or child nodes.'"/>
      <details key="ActivationIntegrity" value="self.isActivated = true implies self.activationLevel >= self.minActivationLevel and self.activationLevel &lt;= self.maxActivationLevel"/>
      <details key="ActivationIntegrity$message" value="'Activated nodes must have activation level between minimum and maximum activation levels.'"/>
      <details key="MinimalNodeActivation" value="self.isActivated = false implies self.activation->size() = (self.maxActivationLevel - self.minActivationLevel + 1) and self.activationLevel &lt; self.minActivationLevel"/>
      <details key="MinimalNodeActivation$message" value="'Non-activated nodes must have at least one node activation rule and have activation level below minimum.'"/>
      <details key="MaximalNodeActivation" value="self.isActivated = true implies self.activation->size() = (self.maxActivationLevel - self.activationLevel)"/>
      <details key="MaximalNodeActivation$message" value="'Activated nodes must have proper number of node activations.'"/>
      <details key="UniqueNodeActivationLevels" value="self.activation->forAll(n1: NodeActivation, n2: NodeActivation | n1 &lt;> n2 implies n1.level &lt;> n2.level)"/>
      <details key="UniqueNodeActivationLevels$message" value="'All node activations must have unique levels.'"/>
      <details key="ProperNodeActivationLevels" value="&#xA;&#x9;&#x9;if self.isActivated = true then&#xA;&#x9;&#x9;&#x9;self.activation->forAll(n: NodeActivation | n.level > self.activationLevel and n.level &lt;= self.maxActivationLevel)&#xA;&#x9;&#x9;else&#xA;&#x9;&#x9;&#x9;self.activation->forAll(n: NodeActivation | n.level >= self.minActivationLevel and n.level &lt;= self.maxActivationLevel)&#xA;&#x9;&#x9;endif"/>
      <details key="ProperNodeActivationLevels$message" value="'All node activations must have levels between min and max activation levels.'"/>
      <details key="ProperAbilityLevels" value="self.abilities->forAll(a: Ability | a.onLevel >= self.minActivationLevel and a.onLevel &lt;= self.maxActivationLevel)"/>
      <details key="ProperAbilityLevels$message" value="'All abilities must reference an appropriate node level.'"/>
      <details key="ProperEffectLevels" value="self.abilities->forAll(a: Ability | a.effects->forAll(e: Effect | e.onLevel >= self.minActivationLevel and e.onLevel &lt;= self.maxActivationLevel))"/>
      <details key="ProperEffectLevels$message" value="'All ability effects must reference an appropriate node level.'"/>
    </eAnnotations>
