<?xml version="1.0" encoding="ASCII"?>
<rpg_meta:RPG
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:rpg_meta="rpg_meta"
    xsi:schemaLocation="rpg_meta rpg.ecore"
    name="Dubious DOOB"
    lore="Follow the adventures of the ERP team in order to survive the great invasion from Dubai.">
  <allClasses
      name="Character"
      lore=""
      statistics="//@allStatistics.0 //@allStatistics.1"
      description="All playable characters in the game."
      childClasses="//@allClasses.1 //@allClasses.2 //@allClasses.3">
    <branches
        name="Character Ability Branch"
        description="Abilities"
        branchType="ABILITY"
        tooltip="Abilities">
      <startNodes
          name="Sign into DoobTimer"
          tooltip="Clock yourself in!"
          description="Signs you into DoobTimer, making sure you are at your workplace in a timely fashion"
          nodeType="ROOT"
          activationLevel="1"
          isActivated="true"/>
    </branches>
  </allClasses>
  <allClasses
      name="Programmer"
      lore=""
      statistics="//@allStatistics.3"
      description="Solves development-related tasks. Some complex, other not so much."
      parentClasses="//@allClasses.0">
    <branches
        name="Programmer Ability Branch"
        description="Abilities"
        branchType="ABILITY"
        tooltip="Abilities">
      <startNodes
          name="Code"
          tooltip="Code a bit on a standard project."
          description="The programmer codes on an assigned project."
          nodeType="ROOT"
          maxActivationLevel="5"
          activationLevel="1"
          isActivated="true"/>
    </branches>
  </allClasses>
  <allClasses
      name="Designer"
      description="Designs user interfaces and MPCE pamflets. Because why not."
      parentClasses="//@allClasses.0">
    <branches
        name="Designer Ability Branch"
        description="Abilities"
        branchType="ABILITY"
        tooltip="Abilities">
      <startNodes
          name="Design"
          lore=""
          tooltip="Design an UI item."
          description="The designer designs an item of a user interface on an assigned project."
          nodeType="ROOT"
          activationLevel="1"
          isActivated="true"/>
    </branches>
  </allClasses>
  <allClasses
      name="Animator"
      description="Animates 3D models. Does not sleep."
      parentClasses="//@allClasses.0">
    <branches
        name="Animator Abilities Branch"
        description="Abilities"
        branchType="ABILITY"
        tooltip="Abilities">
      <startNodes
          name="Animate"
          tooltip="Animate a 3D model"
          description="The animator animates a 3D model on an assigned project."
          nodeType="ROOT"
          activationLevel="1"
          isActivated="true"/>
    </branches>
  </allClasses>
  <allStatistics
      name="Health"
      lore=""
      description="The amount of damage a character can sustain before dying."
      tooltip="Amount of current health."
      minValue="1.0"
      maxValue="25000.0"
      startLimitValue="150.0"/>
  <allStatistics
      name="Level"
      description="The amout of progress a character has reached."
      tooltip="Shows how much a character has progressed along the leveling path."
      minValue="1.0"
      maxValue="80.0"
      startLimitValue="1.0"/>
  <allStatistics
      name="Experience"
      description="Amount of experience a player has."
      tooltip="Amount of experience a player has"
      maxValue="10000.0"
      startLimitValue="100.0"/>
  <allStatistics
      name="Coding Mojo"
      description="psudhgpuaierg"
      tooltip="Primary resource for unlocking programmer abilities."/>
</rpg_meta:RPG>
