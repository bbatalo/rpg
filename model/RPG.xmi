<?xml version="1.0" encoding="ASCII"?>
<rpg_meta:RPG
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:rpg_meta="rpg_meta"
    xsi:schemaLocation="rpg_meta rpg.ecore"
    name="Dubious DOOB"
    lore="Follow the adventures of the ERP team in order to survive the great invasion from Dubai.">
  <allClasses
      name="Character"
      lore=""
      statistics="//@allStatistics.0 //@allStatistics.1 //@allStatistics.2 //@allStatistics.3 //@allStatistics.7"
      description="All playable characters in the game."
      childClasses="//@allClasses.1">
    <branches
        name="Character Ability Branch"
        description="Abilities"
        branchType="ABILITY"
        tooltip="Abilities">
      <startNodes
          name="DoobTimer"
          tooltip="Clock yourself in!"
          description="Signs you into DoobTimer, making sure you are at your workplace in a timely fashion"
          nodeType="ROOT"
          maxActivationLevel="3"
          activationLevel="1"
          isActivated="true">
        <activation
            xsi:type="rpg_meta:NodeActivationBuy"
            text="Level 2 Activation"
            level="2">
          <costs
              cost="20.0"
              statistic="//@allStatistics.3"/>
        </activation>
        <activation
            xsi:type="rpg_meta:NodeActivationBuy"
            text="Level 3 Activation"
            level="3">
          <costs
              cost="30.0"
              statistic="//@allStatistics.3"/>
        </activation>
        <abilities
            name="Sign in"
            lore="Sign in"
            tooltip="Sign in"
            castType="CAST"
            castTime="2.0"
            range="1.0"
            rangeUnit="//@rangeUnits.0"
            onLevel="1">
          <effects
              name="Basic Sign In"
              tooltip="Signs in"
              effectType="ACTIVE"
              onLevel="1">
            <statChange
                baseValue="5.0"
                tickValue="1"
                changesStatistic="//@allStatistics.3">
              <modifiers
                  value="1.0"
                  basedOnModifier="MULTIPLY"
                  priorityOrder="1"
                  basedOn="//@allStatistics.1"
                  basedOnValue="2.0"/>
            </statChange>
          </effects>
          <effects
              name="Signed in!"
              tooltip="Signed in!"
              onLevel="1">
            <statChange
                tickType="PER_TICK"
                baseValue="1.0"
                tickValue="480"
                changesStatistic="//@allStatistics.7"/>
          </effects>
        </abilities>
        <abilities
            name="Sign Out"
            lore="Sign Out"
            tooltip="Sign Out"
            castType="CAST"
            castTime="5.0"
            range="1.0"
            rangeUnit="//@rangeUnits.0"
            onLevel="1">
          <effects
              name="Sign Out"
              tooltip="Sign Out"
              effectType="ACTIVE"
              onLevel="1">
            <statChange
                baseValue="5.0"
                tickValue="1"
                changesStatistic="//@allStatistics.3"/>
          </effects>
        </abilities>
      </startNodes>
    </branches>
  </allClasses>
  <allClasses
      name="Programmer"
      lore=""
      statistics="//@allStatistics.4 //@allStatistics.5 //@allStatistics.6"
      description="Solves development-related tasks. Some complex, other not so much."
      parentClasses="//@allClasses.0">
    <branches
        name="Programmer Ability Branch"
        description="Abilities"
        branchType="ABILITY"
        tooltip="Abilities">
      <startNodes
          name="Coding"
          tooltip="Able to code."
          description="The programmer can code on assigned (and not assigned) projects."
          nodeType="ROOT"
          maxActivationLevel="3"
          activationLevel="1"
          isActivated="true">
        <activation
            xsi:type="rpg_meta:NodeActivationBuy"
            text="Level 2 Activation"
            level="2">
          <costs
              cost="10.0"
              statistic="//@allStatistics.4"/>
        </activation>
        <activation
            xsi:type="rpg_meta:NodeActivationBuy"
            text="Level 3 Activation"
            level="3">
          <costs
              statistic="//@allStatistics.4"/>
        </activation>
        <abilities
            name="Code"
            lore="Code"
            tooltip="Code"
            castType="CHANNELED"
            castTime="60.0"
            range="1.0"
            rangeUnit="//@rangeUnits.0"
            onLevel="1">
          <effects
              name="ssss"
              tooltip="ssss"
              onLevel="1"/>
        </abilities>
      </startNodes>
      <startNodes
          name="Bugs!"
          tooltip="Bugs!"
          description="Bugs!"
          nodeType="ROOT">
        <activation
            xsi:type="rpg_meta:NodeActivationPropagate"
            text="Level 1 Activation"
            isAuto="true"
            level="1"
            followsActivationOf="//@allClasses.1/@branches.0/@startNodes.0"
            requiredLevel="2"/>
        <abilities
            name="Report a Bug"
            lore="Report a Bug"
            tooltip="Report a Bug"
            castType="CHANNELED"
            abilityType="AOE"
            castTime="15.0"
            range="10.0"
            rangeUnit="//@rangeUnits.0"
            onLevel="1">
          <effects
              name="ssss"
              tooltip="ssssss"
              onLevel="1"/>
        </abilities>
      </startNodes>
    </branches>
  </allClasses>
  <allStatistics
      name="Health"
      lore=""
      description="The amount of damage a character can sustain before dying."
      tooltip="Amount of current health."
      minValue="1.0"
      maxValue="25000.0"
      startLimitValue="150.0"/>
  <allStatistics
      name="Level"
      description="The amout of progress a character has reached."
      tooltip="Shows how much a character has progressed along the leveling path."
      minValue="1.0"
      maxValue="80.0"
      startLimitValue="1.0"/>
  <allStatistics
      name="Experience"
      description="Amount of experience a player has."
      tooltip="Amount of experience a player has"
      maxValue="10000.0"
      startLimitValue="100.0"/>
  <allStatistics
      name="ERP Coin"
      description="psudhgpuaierg"
      tooltip="Primary resource for unlocking programmer abilities."
      maxValue="100.0"
      startLimitValue="100.0"/>
  <allStatistics
      name="Janko Coin"
      description="Coins that Janko gives out"
      tooltip="Coins that Janko gives out"
      maxValue="100.0"
      startLimitValue="100.0"/>
  <allStatistics
      name="Programming Task"
      description="Programming Task"
      tooltip="Programming Task"
      maxValue="999.0"
      startLimitValue="5.0"/>
  <allStatistics
      name="Reported Bug"
      description="Reported Bug"
      tooltip="Reported Bug"
      maxValue="999.0"
      startLimitValue="10.0"/>
  <allStatistics
      name="VREME"
      lore="VREME"
      description="VREME"
      tooltip="VREME"
      maxValue="40.0"
      startLimitValue="40.0"/>
  <rangeUnits
      name="Meter"
      description="One meter of real life range."/>
</rpg_meta:RPG>
